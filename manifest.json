{
    "name": "Hell Naw Blocker",
    "version": "1.2",
    "description": "Block URLs",
    "permissions": [
        "tabs",
        "activeTab",
        "http://*/",
        "https://*/",
        "storage"
    ],
    "icons": {
        "16": "images/icon_16.png",
        "32": "images/icon_32.png",
        "48": "images/icon_48.png",
        "128": "images/icon_128.png"
    },
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}